{
  "name": "teach-code-stencil",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil App Starter",
  "scripts": {
    "build": "stencil build",
    "buildProd": "stencil build --prod",
    "start": "stencil build --dev --watch --serve",
    "asProd": "stencil build --local --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "deploy": "npm run buildProd && npm run copy && npm run copyHtml && gh-pages -d dist",
    "copy": "copyfiles -u 1 www/dist/** ./",
    "copyHtml": "copyfiles index.html dist",
    "upload": "gh-pages -d dist"
  },
  "devDependencies": {
    "@stencil/core": "^1.12.2",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@types/node": "^14.0.23",
    "gh-pages": "^3.1.0"
  },
  "license": "MIT"
}
